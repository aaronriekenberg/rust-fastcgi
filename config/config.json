{
    "server_configuration": {
        "server_type": "UNIX",
        "bind_address": "/var/www/run/go-fastcgi/socket",
        "fastcgi_connection_configuration": {
            "max_concurrent_connections": 10,
            "max_requests_per_connection": 10
        }
    },
    "command_configuration": {
        "max_concurrent_commands": 10,
        "semaphore_acquire_timeout_millis": 200,
        "commands": [
            {
                "id": "chronyc_sources",
                "description": "chronyc sources",
                "command": "/usr/bin/chronyc",
                "args": [
                    "-n",
                    "sources",
                    "-a",
                    "-v"
                ]
            },
            {
                "id": "df",
                "description": "df",
                "command": "/usr/bin/df",
                "args": [
                    "-h"
                ]
            },
            {
                "id": "ip_addr",
                "description": "ip addr",
                "command": "/usr/sbin/ip",
                "args": [
                    "addr"
                ]
            },
            {
                "id": "lscpu",
                "description": "lscpu",
                "command": "/usr/bin/lscpu"
            },
            {
                "id": "netstat_an",
                "description": "netstat -an",
                "command": "/usr/bin/netstat",
                "args": [
                    "-a",
                    "-n"
                ]
            },
            {
                "id": "pitemp",
                "description": "pitemp",
                "command": "/home/aaron/repo/raspberrypi/pitemp.sh"
            },
            {
                "id": "top",
                "description": "top",
                "command": "/usr/bin/top",
                "args": [
                    "-b",
                    "-n1"
                ]
            },
            {
                "id": "uptime",
                "description": "uptime",
                "command": "/usr/bin/uptime"
            },
            {
                "id": "vmstat",
                "description": "vmstat",
                "command": "/usr/bin/vmstat"
            },
            {
                "id": "w",
                "description": "w",
                "command": "/usr/bin/w"
            }
        ]
    }
}