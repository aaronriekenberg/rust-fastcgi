{
    "context_configuration": {
        "context": "/cgi-bin"
    },
    "server_configuration": {
        "server_type": "UNIX",
        "bind_address": "./socket",
        "fastcgi_connection_configuration": {
            "max_concurrent_connections": 10,
            "max_requests_per_connection": 10
        }
    },
    "command_configuration": {
        "max_concurrent_commands": 1,
        "semaphore_acquire_timeout_millis": 200,
        "commands": [
            {
                "id": "sleep",
                "description": "sleep",
                "command": "/bin/sleep",
                "args": [
                    "5"
                ]
            },
            {
                "id": "vmstat",
                "description": "vmstat",
                "command": "/usr/bin/vmstat"
            },
            {
                "id": "w",
                "description": "w",
                "command": "/usr/bin/w"
            }
        ]
    }
}